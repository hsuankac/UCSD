void myDelay1(uint32_t val) {
	// Wait for the specified number of milliseconds
	htim2->Instance->CNT = 0;
	htim2->Instance->CR1 = 1;
	while (__HAL_TIM_GET_COUNTER(&htim2) < val);  // wait for the counter to reach the us input in the parameter
	htim2->Instance->CR1 = 0;
}

void myDelay2(uint32_t val) {
	uint32_t tickstart = HAL_GetTick();

	while ((HAL_GetTick() - tickstart) < val)
	{
	}
}

void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim)
{
	if(htim == &htim3)
	{
		char buf[100];
		tim3Count++;
		snprintf(buf, sizeof(buf), "Counted timer3 event = %d\r\n", tim3Count);
		logMsg(&huart1, buf);
	}

}

void HAL_RTC_AlarmAEventCallback(RTC_HandleTypeDef *hrtc)
{
	logMsg(&huart1, "RTC alarm A detected .... \r\n");

}

